      <O2PAGPARS>
       <APPLNAME>ZGEOCODE_MOBILE</APPLNAME>
       <PAGEKEY>MAP.XLOC</PAGEKEY>
       <COMPNAME>GEODATA</COMPNAME>
       <VERSION>A</VERSION>
       <PARDECLTYP>1</PARDECLTYP>
