<PROG NAME="ZGEOCODE_NOMINATIM_TEST" VARCL="X" SUBC="1" CNAM="BCUSER" CDAT="20100609" UNAM="BCUSER" UDAT="20101230" VERN="000026" RSTAT="T" RMAND="000" RLOAD="E" FIXPT="X" SDATE="20101230" STIME="001746" IDATE="20101230" ITIME="001746" UCCHECK="X">
 <textPool>
  <language SPRAS="E">
   <textElement ID="R" ENTRY="Test ZGEOCODE_NOMINATIM" LENGTH="23 "/>
